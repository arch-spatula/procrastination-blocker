{
  "manifest_version": 3,
  "name": "procrastination blocker",
  "version": "0.0.1",
  "description": "방해되는 사이트 차단",
  "icons": {
    "48": "images/icon-48.svg",
    "128": "images/icon-128.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "https://*.netflix.com/*",
        "https://*.twitch.tv/*",
        "https://chzzk.naver.com/*"
      ],
      "js": ["index.js"]
    }
  ]
}
